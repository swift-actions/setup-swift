{
  "name": "setup-swift",
  "version": "1.0.0",
  "description": "Set up GitHub Actions workflow with Swift support",
  "private": true,
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "pack": "ncc build",
    "pack-source-map": "ncc build --source-map",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest",
    "all": "npm run format && npm run build && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fwal/setup-swift.git"
  },
  "keywords": [
    "github",
    "actions",
    "swift",
    "ci",
    "swiftenv"
  ],
  "author": "Frederik Wallner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fwal/setup-swift/issues"
  },
  "homepage": "https://github.com/fwal/setup-swift#readme",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@actions/exec": "^1.0.4",
    "@actions/io": "^1.0.2",
    "@actions/tool-cache": "^1.5.5",
    "getos": "^3.2.1",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/getos": "^3.0.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.4",
    "@types/semver": "^7.2.0",
    "@zeit/ncc": "^0.22.2",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.3"
  }
}
