{
  "name": "setup-swift",
  "version": "1.15.0",
  "description": "Set up GitHub Actions workflow with Swift support",
  "private": true,
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "pack": "ncc build",
    "pack-source-map": "ncc build --source-map",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "test": "jest",
    "all": "npm run format && npm run build && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swift-actions/setup-swift.git"
  },
  "keywords": [
    "github",
    "actions",
    "swift",
    "ci",
    "swiftenv"
  ],
  "author": "Frederik Wallner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swift-actions/setup-swift/issues"
  },
  "homepage": "https://github.com/swift-actions/setup-swift#readme",
  "dependencies": {
    "@actions/core": "^1.9.0",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^1.7.2",
    "getos": "^3.2.1",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@types/getos": "^3.0.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.32",
    "@types/semver": "^7.3.9",
    "@zeit/ncc": "^0.22.3",
    "jest": "^26.6.3",
    "prettier": "^2.6.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.6.4"
  }
}
